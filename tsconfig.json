{
  "extends": "@tsconfig/strictest/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "react",
    "lib": ["DOM", "ESNext"],
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "sourceMap": true,
    "resolveJsonModule": true,
    "target": "ESNext"
  }
}
